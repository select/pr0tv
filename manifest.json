{
  "manifest_version": 2,
  "name": "pr0-tv",
  "minimum_chrome_version": "50.0.0.0",
  "version": "0.7",
  "content_scripts": [
    {
      "matches": ["https://pr0gramm.com/*", "http://pr0gramm.com/*"],
      "js": ["pr0-tv.user.js"],
      "css": ["style.css", "pr0tv.css"]
    }
  ],
  "web_accessible_resources": [
    "fonts/*"
  ],
  "icons": {
    "16": "pr0tv-logo16.png",
    "48": "pr0tv-logo48.png",
    "128": "pr0tv-logo128.png"
  }
}